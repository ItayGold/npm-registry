{
  "name": "@itay.gold/npm-registry",
  "version": "1.5.2",
  "bin": {
    "click": "./itay-gold-cli.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "click": "click",
    "conventional-changelog": "conventional-changelog",
    "read:docs": "open http://npm-docs.clicksoftware.com",
    "click:pub": "click publish --help",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "stats": "click analyze -p $project",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run npm-registry:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "compile:server": "tsc -p server.tsconfig.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@nguniversal/common": "^8.2.6",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-translate/core": "^11.0.1",
    "bootstrap": "^4.5.0",
    "commander": "^2.20.3",
    "core-js": "^2.6.11",
    "express": "^4.15.2",
    "moment": "^2.27.0",
    "ngx-bootstrap": "^5.6.1",
    "ngx-mask": "^7.9.8",
    "ngx-perfect-scrollbar": "^8.0.0",
    "rxjs": "^6.6.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular-devkit/build-ng-packagr": "^0.802.2",
